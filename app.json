{
  "addons": [
    "mongohq",
    "newrelic",
    "heroku-redis",
    "scheduler"
  ],
  "buildpacks": [
    {
      "url": "heroku/ruby"
    },
    {
      "url": "https://github.com/dscout/wkhtmltopdf-buildpack.git"
    }
  ],
  "env": {
    "LANG": {
      "required": true
    },
    "MONGO_URL": {
      "required": true
    },
    "MONGOHQ_URL": {
      "required": true
    },
    "NEW_RELIC_LICENSE_KEY": {
      "required": true
    },
    "NEW_RELIC_LOG": {
      "required": true
    },
    "RACK_ENV": {
      "required": true
    },
    "RAILS_ENV": {
      "required": true
    },
    "RAILS_LOG_TO_STDOUT": {
      "required": true
    },
    "RAILS_SERVE_STATIC_FILES": {
      "required": true
    },
    "REDIS_URL": {
      "required": true
    },
    "SECRET_KEY_BASE": {
      "required": true
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    }
  },
  "name": "backoffice",
  "scripts": {},
  "stack": "heroku-18"
}